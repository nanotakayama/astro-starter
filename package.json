    "prettier-plugin-tailwindcss": "^0.5.14",
    "rimraf": "^5.0.5"
  },
  "dependencies": {
    "@astrojs/ts-plugin": "^1.10.4",
    "astro-html-beautifier": "^1.0.4",
    "motion": "^11.11.17",
