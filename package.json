{
  "name": "astro4.0-starter",
  "description": "front-end development environment.",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "npx astro dev",
    "start": "npx astro dev",
    "build": "astro check && astro build && prettier --write ./public/**/*.html --ignore-path .prettierignore",
